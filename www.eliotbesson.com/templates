<script>
document.addEventListener("DOMContentLoaded", function () {
let mm = gsap.matchMedia();

mm.add("(min-width: 992px)", () => {
  document.fonts.ready.then(() => {
    gsap.set("[data-hidden]", { visibility: "visible" });

    const subtitleSplit = new SplitType(
      ".template-hero_subtitle-wrap .txt-color-light-60o", {
        types: "words, lines"
      });

    const tl = gsap.timeline();
    tl.from("[heading-1]", {
        opacity: 1,
        scale: 0.5,
        duration: 1,
        ease: "expo.out",
      }, 0)

      .from("[heading-2]", {
        opacity: 1,
        scale: 0.5,
        duration: 1,
        ease: "expo.out",
      }, 0)

      .from("[heading-1-wrap]", {
        y: "0em",
        x: "-6em",
        opacity: 0.2,
        scale: 0.7,

        duration: 1.6,
        ease: "expo.inOut",
      }, 0.2)

      .from("[heading-2-wrap]", {
        y: "0em",
        scale: 0.7,
        x: "6em",
        opacity: 0.2,
        duration: 1.6,
        ease: "expo.inOut",
      }, 0.2)

      .from(".template_video-w", {
        scale: 0,
        opacity: 0,
        ease: 'expo.out',
        duration: 0.8,

      }, 1)

      .from(subtitleSplit.lines, {
        y: "100%",
        opacity: 0,
        duration: 0.8,
        ease: "power4.out",
        stagger: 0.1,
      }, 1)

      .from(".template-hero_marquee-w", {
        opacity: 0,
        scale: 1.1,
        ease: 'power4.out',
        duration: 1,
      }, 1.2)

      .from(
        ".template_cms .template_item:nth-child(1), .template_cms .template_item:nth-child(2)", {
          y: 240,
          opacity: 0,
          ease: 'expo.out',
          duration: 1.2,
          stagger: 0.15
        }, 1.2)

      .from(".template_filters-c", {
        y: "20vw",
        duration: 1,
        ease: "expo.out",
      }, 1.4)

      .from(".menu_logo-link", {
        x: "-50vw",
        scale: 1.2,
        opacity: 0,
        duration: 1.2,
        ease: "expo.out",
      }, 1.4)

      .from(".menu_links .menu_link", {
        y: "-10vw",
        duration: 1.2,
        ease: "expo.out",
        stagger: {
          each: 0.05,
          from: "start"
        }
      }, 1.4)

      .from(".menu_main-inner", {
        x: "50vw",
        scale: 1.2,
        opacity: 0,
        duration: 1.2,
        ease: "expo.out",
      }, 1.4)

      .set(".template-hero_marquee-w", { pointerEvents: "auto" }, 1.4)
      .set(".template-hero_section", { cursor: "auto" }, 1.4);

  });

  return () => gsap.killTweensOf("*");
});

mm.add("(max-width: 991px)", () => {
  document.fonts.ready.then(() => {
    gsap.set("[data-hidden]", { visibility: "visible" });

    const subtitleSplit = new SplitType(
      ".template-hero_subtitle-wrap .txt-color-light-60o", {
